<table class="property-table">
  <tr>
    <th class="property-table">Title:</th>
    <td><%= metadata.title %>
  </tr>

  <tr>
    <th class="property-table">Description:</th>
    <td><%= metadata.description %>
  </tr>

  <tr>
    <th class="property-table">Resource Type:</th>
    <td><%= metadata.resource_type %>
  </tr>

  <tr>
    <th class="property-table">Language:</th>
    <td><%= metadata.language %>
  </tr>

  <tr>
    <th class="property-table">Creator:</th>
    <td><%= vcard_label(metadata.creator) %>
  </tr>

  <tr>
    <th class="property-table">Publisher:</th>
    <td><%= vcard_label(metadata.publisher) %>
  </tr>

  <tr>
    <th class="property-table">Conforms To:</th>
    <td><%= metadata.conformsto.uri %>
  </tr>

  <tr>
    <th class="property-table">Created:</th>
    <td><%= metadata.created %>
  </tr>

  <tr>
    <th class="property-table">Modified:</th>
    <td><%= metadata.modified %>
  </tr>


</table>


