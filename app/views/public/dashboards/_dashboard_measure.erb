<tr>
	<td <%= "style='cursor: help;' title='#{measure['description']}'" if measure['description'] %>><%= measure["measure"] %></td>

	<% if measure['values'].length > 1 %>
		<td><%= measure['values'][measure['values'].size - 2] %></td>
		<td><%= measure['values'].last %></td>
		<td><%= measure["rank"] %></td>
		<td <%= "class='#{measure['visual']}'" if measure['visual'] %>><%= measure['values'].join(',') %></td>
	<% else %>
		<td>&nbsp;</td>
		<td>&nbsp;</td>
		<td>&nbsp;</td>
		<td>&nbsp;</td>
	<% end %>
</tr>
